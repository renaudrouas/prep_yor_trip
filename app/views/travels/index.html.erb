<div class="content">
  <div class="cards-container">
    <div class="col-xs-12 col-sm-7">
      <div class="row">
        <div class="col-xs-12">
          <div class="bandeau">
            <p>bandeau</p>
          </div>
        </div>
      </div>
      <div class="col-xs-12">
        <div class="table-responsive">
          <table class="table">
            <tbody>
              <tr>
                <td></td>
                <td><h3>Depart - Arrivée</h3></td>
                <td><h3>Nom de la ville - Pays</h3></td>
              </tr>
              <tr>
                <td><%= image_tag "hotel.png"%></td>
                <td><h3>travel.date.depart</h3></td>
                <td><h3>travel.country</h3></td>
              </tr>
              <tr>
                <td></td>
                <td><h3>Adress</h3></td>
                <td><h3>travel.adress</h3></td>
              </tr>
              <tr>
                <td><a href="" class="btn btn-trip">EDIT</a></td>
                <td><h3>Hotel Name</h3></td>
                <td><h3>travel.hotel.name</h3></td>
              </tr>
              <tr>
                <td></td>
                <td><h3>Phone number</h3></td>
                <td><h3>travel.phone.number</h3></td>
              </tr>
              <tr>
                <td><a href="" class="btn btn-transport">DELETE</a></td>
                <td><h3>Email</h3></td>
                <td><h3>travel.email</h3></td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
  <!-- <div id="map"></div> -->
</div>

<script>
      function initMap() {
        // Create a map object and specify the DOM element for display.
        var map = new google.maps.Map(document.getElementById('map'), {
          center: {lat: 52.5206, lng: 13.4098},
          scrollwheel: false,
          zoom: 8
        });
      }
    </script>

<script async defer
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCzER7O10ZOUp7FloJzMe2r8X1GIQhgt7Q&callback=initMap">
        </script>

